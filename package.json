{
  "name": "create-react-meteor-app",
  "description": "Create React-Meteor apps with no build configuration.",
  "version": "0.0.1",
  "scripts": {
    "start": "meteor run"
  },
  "dependencies": {
    "@babel/runtime": "7.0.0-beta.55",
    "babel-runtime": "^6.26.0",
    "core-js": "2.5.7",
    "meteor-node-stubs": "0.4.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-meteor-data": "^0.2.11",
    "react-router": "^4.3.1",
    "react-router-auth": "^1.0.1",
    "react-router-dom": "^4.3.1",
    "simpl-schema": "^1.5.3",
    "uniforms": "^1.25.0",
    "uniforms-unstyled": "^1.25.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.1.1",
    "css-mqpacker": "^7.0.0",
    "postcss": "^6.0.22",
    "postcss-csso": "^3.0.0",
    "postcss-load-config": "^1.2.0"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "browsers": [
          ">2%",
          "last 3 versions",
          "Firefox ESR",
          "not ie < 11"
        ]
      },
      "css-mqpacker": {},
      "postcss-csso": {}
    }
  }
}
